        },
        {
            "@id": "trov:IncludeAllCode",
            "@type": "trov:TROPolicy",
            "trov:hasPolicyName": "IncludeAllCode",
            "trov:hasPolicyDescription": "TRO includes all code."
        },
        {
            "@id": "trov:DescribeSoftwareEnvironment",
            "@type": "trov:TROPolicy",
            "trov:hasPolicyName": "DescribeSoftwareEnvironment",
            "trov:hasPolicyDescription": "TRO describes the full software environment."
        },
        {
            "@id": "trov:PreventRuntimeIntervention",
            "@type": "trov:TRSPolicy",
            "trov:hasPolicyName": "PreventRuntimeIntervention",
            "trov:hasPolicyDescription": "TRS prevents runtime intervention."
        }
    ]
}
