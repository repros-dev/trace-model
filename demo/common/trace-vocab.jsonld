{
    "@context": [{
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "trov": "http://trace.org/trov#"
    }],

    "@graph": [
        {
            "@id": "trov:Policy",
            "@type": "rdfs:Class"
        },
        {
            "@id": "trov:TROPolicy",
            "@type": "rdfs:Class",
            "rdfs:subClassOf": { "@id": "trov:Policy" }
        },
        {
            "@id": "trov:TRSPolicy",
            "@type": "rdfs:Class",
            "rdfs:subClassOf": { "@id": "trov:Policy" }
        },
        {
            "@id": "trov:enforcesPolicy",
            "@type": "rdf:Property"
        },
        {
            "@id": "trov:enforcesTROPolicy",
            "@type": "rdf:Property",
            "rdfs:subPropertyOf": { "@id": "trov:enforcesPolicy" }
        },
        {
            "@id": "trov:enforcesTRSPolicy",
            "@type": "rdf:Property",
            "rdfs:subPropertyOf": { "@id": "trov:enforcesPolicy" }
        },


        {
            "@id": "trov:IncludeAllOutput",
